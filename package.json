{
  "name": "emr",
  "version": "1.0.0",
  "description": "Full React Application",
  "main": "index.js",
  "scripts": {
    "serve": "cd dist && serve",
    "build": "webpack --config production.webpack.config.js",
    "test": "cross-env NODE_ENV=test mocha-webpack --webpack-config ./test.webpack.config.js --require /test/helpers/browser.js \"emr-fe/test/**/*.spec.js\"",
    "coverage": "cross-env NODE_ENV=test nyc mocha-webpack --webpack-config ./test.webpack.config.js --require /test/helpers/browser.js \"emr-fe/test/**/*.spec.js\"",
    "coverage:watch": "npm-watch",
    "watch": "webpack --watch",
    "test:watch": "cross-env NODE_ENV=test mocha-webpack --webpack-config ./test.webpack.config.js --require /test/helpers/browser.js \"emr-fe/test/**/*.spec.js\" --watch",
    "test:tap": "cross-env NODE_ENV=test mocha-webpack --webpack-config ./test.webpack.config.js --require /test/helpers/browser.js \"emr-fe/test/**/*.spec.js\" --reporter tap > fe-unit-test.tap",
    "test:junit": "cross-env NODE_ENV=test mocha-webpack --webpack-config ./test.webpack.config.js --require /test/helpers/browser.js \"emr-fe/test/**/*.spec.js\" --reporter mocha-junit-reporter --reporter-options mochaFile=./fe-test-results.xml",
    "test:ui": "wdio wdio.conf.js",
    "doc:build": "gitbook build",
    "storybook": "start-storybook -p 9090",
    "lint": "eslint",
    "build-storybook": "build-storybook"
  },
  "watch": {
    "coverage": "emr-fe/test/**/*.spec.js"
  },
